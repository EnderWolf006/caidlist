{
    "files.associations": {
        "*.json": "jsonc",
        "*.lang": "properties"
    },
    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "html"
    ],
    "json.schemas": [
        {
            "fileMatch": [
                "*.json"
            ],
            "schema": {
                "allowTrailingCommas": true
            }
        }
    ],
}